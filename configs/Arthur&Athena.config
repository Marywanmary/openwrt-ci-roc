# === 基础TARGET配置 ===
CONFIG_TARGET_qualcommax=y
CONFIG_TARGET_qualcommax_ipq60xx=y
CONFIG_TARGET_MULTI_PROFILE=y

# === 设备启用 ===
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01=y
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02=y

# === 设备特定包 ===
#CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02+="luci-app-athena-led luci-i18n-athena-led-zh-cn"

# === 包组定义 ===
CONFIG_PACKAGE_GROUP_BASE="netspeedtest nikki openclash partexp samba4 smbuser tailscale taskplan ttyd upnp zram-swap"
CONFIG_PACKAGE_GROUP_ATHENA_LITE="easytier homeproxy lucky"
CONFIG_PACKAGE_GROUP_ATHENA_FULL="ddns-go istorex"

# === 启用子配置 ===
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01_lite=y
CONFIG_TARGET_DEVICE_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02_lite=y

# === 亚瑟系列配置 ===
# 亚瑟Lite
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01_lite=""
# 亚瑟Full(雅典娜Lite)
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-ss-01="luci-app-easytier luci-app-homeproxy luci-app-lucky"

# === 雅典娜系列配置 ===
# 雅典娜Lite(与亚瑟Full相同)
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02_lite="luci-app-easytier luci-app-homeproxy luci-app-lucky"
# 雅典娜Full
CONFIG_TARGET_DEVICE_PACKAGES_qualcommax_ipq60xx_DEVICE_jdcloud_re-cs-02="luci-app-ddns-go luci-app-istorex"


# === 编译优化 ===
CONFIG_DEVEL=y
CONFIG_TARGET_OPTIONS=y
CONFIG_TARGET_OPTIMIZATION="-O3 -pipe -march=armv8-a+crypto+crc -mcpu=cortex-a53+crypto+crc -mtune=cortex-a53"

# === IQP60XX优化 ===
CONFIG_ATH11K_MEM_PROFILE_512M=y
CONFIG_NSS_FIRMWARE_VERSION_12_5=y

# === 雅典娜LED ===
#CONFIG_PACKAGE_luci-app-athena-led=m
#CONFIG_PACKAGE_luci-i18n-athena-led-zh-cn=m


# ====== Mary定制包 ======

# DDNS-go
CONFIG_PACKAGE_ddns-go=m
CONFIG_PACKAGE_luci-app-ddns-go=m
# EasyTier
CONFIG_PACKAGE_easytier=m
CONFIG_PACKAGE_luci-app-easytier=m
# HomeProxy
CONFIG_PACKAGE_homeproxy=m
CONFIG_PACKAGE_luci-app-homeproxy=m
CONFIG_PACKAGE_luci-i18n-homeproxy-zh-cn=m
# iStoreX
CONFIG_PACKAGE_istorex=m
CONFIG_PACKAGE_luci-app-istorex=m
# Lucky
CONFIG_PACKAGE_lucky=m
CONFIG_PACKAGE_luci-app-lucky=m
